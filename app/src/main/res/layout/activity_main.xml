<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/l1"
        android:layout_width="300dp"
        android:layout_marginLeft="50dp"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:gravity="center"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/back"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"

            android:src="@drawable/baseline_arrow_back_24"
            >

        </ImageView>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="Menu"
            android:textSize="19dp"
            android:textStyle="bold"
            android:gravity="center"
            android:layout_marginRight="20dp"
            android:textColor="@android:color/black"
            />



    </LinearLayout>


    <View
        android:id="@+id/v1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/bleuuu"
        android:layout_below="@+id/l1"
        android:paddingRight="8dp"
        android:paddingLeft="8dp"
        android:layout_marginTop="1dp">

    </View>

<de.hdodenhof.circleimageview.CircleImageView
    android:id="@+id/imgview_profile"

    android:layout_width="120dp"
    android:layout_height="120dp"
    android:src="@mipmap/ic_launcher"
android:layout_centerHorizontal="true"
    android:layout_marginTop="60dp"


    />

    <LinearLayout
        android:id="@+id/l_layout1"
        android:layout_width="300dp"
        android:layout_marginLeft="50dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imgview_profile"
        android:layout_marginTop="63dp"
        android:orientation="horizontal"
        android:background="@drawable/menu">


        <ImageView

            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_below="@+id/nom"
            android:src="@drawable/baseline_person_ayoub"
            android:layout_marginLeft="70dp"

            >


        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="profil"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/l_layout2"
        android:layout_width="300dp"
        android:layout_marginLeft="50dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/l_layout1"
        android:layout_marginTop="20dp"
        android:orientation="horizontal"
        android:background="@drawable/menu">


        <ImageView

            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_below="@+id/nom"
            android:src="@drawable/baseline_history_toggle_off_24"
            android:layout_marginLeft="70dp"

            >


        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="history"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
/>
    </LinearLayout>














    <LinearLayout
        android:id="@+id/layout_addpark"
        android:layout_width="300dp"
        android:layout_marginLeft="50dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/l_layout2"
        android:layout_marginTop="20dp"
        android:orientation="horizontal"
        android:background="@drawable/menu"
        android:visibility="visible"
        >


        <ImageView

            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_below="@+id/nom"
            android:src="@drawable/baseline_add_location_alt_24"
            android:layout_marginLeft="70dp"

            >


        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ajouter un parking"
            android:textSize="18sp"
            android:textColor="@color/white"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"/>

    </LinearLayout>








    <LinearLayout
        android:id="@+id/layout_infoparking"
        android:layout_width="300dp"
        android:layout_marginLeft="50dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/layout_addpark"
        android:layout_marginTop="20dp"
        android:orientation="horizontal"
        android:background="@drawable/menu"
        android:visibility="visible"
        >


        <ImageView

            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_below="@+id/nom"
            android:src="@drawable/baseline_local_parking_24"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="70dp"

            >


        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="information de parking"
            android:textSize="18sp"
            android:textColor="@color/white"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"/>

    </LinearLayout>






    <LinearLayout
        android:id="@+id/l_layout3"
        android:layout_width="300dp"
        android:layout_marginLeft="50dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/layout_infoparking"
        android:layout_marginTop="20dp"
        android:orientation="horizontal"
        android:background="@drawable/menu">


        <ImageView

            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_below="@+id/nom"
            android:src="@drawable/baseline_settings_24"
            android:layout_marginLeft="70dp"

            >


        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="paramÃ¨tre"
            android:textSize="18sp"
            android:textColor="@color/white"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"/>

    </LinearLayout>



















    <LinearLayout
        android:id="@+id/l_layout5"
        android:layout_width="300dp"
        android:layout_marginLeft="50dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/l_layout3"
        android:layout_marginTop="20dp"
        android:orientation="horizontal"
        android:background="@drawable/menu">


        <ImageView

            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_below="@+id/nom"
            android:src="@drawable/baseline_help_24"
            android:layout_marginLeft="70dp"

            >


        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="aide"
            android:textSize="18sp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:textColor="@color/white"
            />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/l_layout4"
        android:layout_width="300dp"
        android:layout_marginLeft="50dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/l_layout3"
        android:layout_marginTop="250dp"
        android:orientation="horizontal"
        android:background="@drawable/menu">


        <ImageView

            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_below="@+id/nom"
            android:src="@drawable/baseline_info_24"
            android:layout_marginLeft="70dp"

            >


        </ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="info"
            android:textSize="18sp"
            android:textColor="@color/white"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"/>

    </LinearLayout>










</RelativeLayout>

