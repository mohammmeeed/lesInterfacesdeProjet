            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ro1" />

        <TextView
            android:id="@+id/ro2"
            android:layout_width="wrap_content"
